<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Username</th>
        <th>Password</th>
    </tr>
    <% if (obj.length) { %>
        <% for(var i = 0; i < obj.length; i++) { %>
            <% var user = obj[i]%>
            <tr>
                <td>
                    <%= user.firstname %>
                </td>
                <td>
                    <%= user.lastname %>
                </td>
                <td>
                    <%= user.email %>
                </td>
                <td>
                    <%= user.username %>
                </td>
                <td>
                    <%= user.password %>
                </td>
            </tr>
        <% }; %>
    <% } %>
</table>


